{"index.html":"<html><head><script src=\"//ej2.syncfusion.com/javascript/demos/grid/row-height/datasource.js\" type=\"text/javascript\"></script>\n<script src=\"https://cdn.syncfusion.com/ej2/dist/ej2.min.js\" type=\"text/javascript\"></script>\n    <link href=\"https://cdn.syncfusion.com/ej2/material.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\"><style>\n@font-face {\nfont-family: 'e-grid-rowheight';\nsrc:\nurl(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');\nfont-weight: normal;\nfont-style: normal;\n}\n.e-grid .e-small-icon::before {\n    font-family: 'e-grid-rowheight';\n    content: '\\e707';\n}\n\n.e-grid .e-medium-icon::before {\n    font-family: 'e-grid-rowheight';\n    content: '\\e701';\n}\n\n.e-grid .e-big-icon::before {\n    font-family: 'e-grid-rowheight';\n    content: '\\e702';\n}\n</style>\n<div>\n    <div class=\"control-section\">\n        <div class=\"content-wrapper\">\n            <div id=\"Grid\"></div>\n        </div>\n    </div>\n</div>\n\n\n\n\n</div></body></html>","index.js":"{{ripple}}/**\n * Row Height sample\n */\n\n    var gridData = new ej.data.DataManager(window.data).executeLocal(new ej.data.Query().take(30));\n    var grid = new ej.grids.Grid({\n        dataSource: gridData,\n        toolbar: [\n            { prefixIcon: 'e-small-icon', id: 'big', align: 'right' },\n            { prefixIcon: 'e-medium-icon', id: 'medium', align: 'right' },\n            { prefixIcon: 'e-big-icon', id: 'small', align: 'right' }\n        ],\n        columns: [\n            { field: 'OrderID', headerText: 'Order ID', width: 120, textAlign: 'Right' },\n            { field: 'CustomerName', headerText: 'Customer Name', width: 150 },\n            { field: 'OrderDate', headerText: 'Order Date', width: 130, format: 'yMd', textAlign: 'Right' },\n            { field: 'Freight', width: 120, format: 'C2', textAlign: 'Right' },\n            { field: 'ShippedDate', headerText: 'Shipped Date', width: 140, format: 'yMd', textAlign: 'Right' },\n            { field: 'ShipCountry', headerText: 'Ship Country', width: 150 }\n        ],\n        height: 400,\n        rowHeight: 20,\n        toolbarClick: function (args) {\n            if (args.item.id === 'small') {\n                grid.rowHeight = 20;\n            }\n            if (args.item.id === 'medium') {\n                grid.rowHeight = 40;\n            }\n            if (args.item.id === 'big') {\n                grid.rowHeight = 60;\n            }\n        }\n    });\n    grid.appendTo('#Grid');\n"}